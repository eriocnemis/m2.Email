<include xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_include.xsd">
    <!-- general contact setting -->
    <group id="ident_general">
        <!-- include transport setting -->
        <include path="Eriocnemis_Email::system/transport.xml"/>
        <!-- include sendmail setting -->
        <include path="Eriocnemis_Email::system/transport/sendmail.xml"/>
        <!-- include smtp setting -->
        <include path="Eriocnemis_Email::system/transport/smtp.xml"/>
    </group>
</include>
